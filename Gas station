class Solution {
    public int canCompleteCircuit(int[] gas, int[] cost) {
        int cur=0;
        int tot=0;
        int st=0;
        for(int i=0;i<gas.length;i++){
            cur+=gas[i]-cost[i];
            tot+=gas[i]-cost[i];
            if(cur<0){
                cur=0;
                st=i+1;
            }
        }
        if(tot<0) return -1;
    return st;
    }
}
